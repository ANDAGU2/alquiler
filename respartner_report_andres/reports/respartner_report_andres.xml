<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <report
                id="report_custom_respartner_andres"
                string="contacto / andres"
                model="res.partner"
                report_type="qweb-pdf"
                name="respartner_report_andres.report_custom_respartner_andres"
        />

        <record id="paperformat_report_custom_respartner_andres" model="report.paperformat">
          <field name="name">Formato de papel contacto andres</field>
          <field name="default" eval="True"/>
          <field name="format">A4</field>
          <field name="page_height">0</field>
          <field name="page_width">0</field>
          <field name="orientation">Portrait</field>
          <field name="margin_top">45</field>
          <field name="margin_bottom">15</field>
          <field name="margin_left">7</field>
          <field name="margin_right">7</field>
          <field name="header_line" eval="False"/>
          <field name="header_spacing">49</field>
          <field name="dpi">90</field>
        </record>

        <template id="report_custom_respartner_andres">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">                    
                     <div class="header">
                      este es el encabezado de andres gutierrez compra
                     </div>
                     <div class="article o_report_layout_standard">
                       <div class="page">
                        este es el cuerpo del qweb compra
                       </div>
                     </div>
                     <div class="footer">
                       este el pie de pagina jajaja
                     </div>
                </t>
            </t>
        </template>

      </data>
</odoo>